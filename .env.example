# 环境配置示例文件
# 复制此文件为 .env.local 并修改相应配置

# 开发环境配置
NODE_ENV=development

# API配置
API_BASE_URL=https://www.vdamo.com
SOCKET_URL=wss://www.vdamo.com/ws

# 测试账号（仅开发环境使用）
TEST_USERNAME=test002
TEST_PASSWORD=123456

# 调试配置
DEBUG_MODE=false
SHOW_CONSOLE_LOGS=false

# APP配置
APP_NAME=DAMO收银系统
APP_VERSION=1.4.5

# 安全配置
ENABLE_HTTPS_ONLY=true
ENABLE_CORS=true